{
  "name": "fantasy-console",
  "version": "0.1.0",
  "description": "A small, deterministic fantasy console for building simple 2D games on the web",
  "type": "module",
  "scripts": {
    "build": "asc cart/minesweeper.ts -o cart/cartridge.wasm --config cart/asconfig.json",
    "build:debug": "asc cart/minesweeper.ts -o cart/cartridge.wasm --config cart/asconfig.json --target debug",
    "serve": "python -m http.server 8080",
    "dev": "npm run build && npm run serve"
  },
  "keywords": [
    "fantasy-console",
    "webassembly",
    "assemblyscript",
    "gamedev"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.28.9"
  }
}
